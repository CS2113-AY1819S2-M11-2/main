<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.ScrollPane?>

<VBox fx:id="calendarCell" alignment="TOP_RIGHT" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <Text fx:id="cellDate" />
    <ScrollPane fx:id="cellTasks" cache="true" VBox.vgrow="ALWAYS">
        <VBox fx:id="cellContent" alignment="CENTER_LEFT" />
    </ScrollPane>
</VBox>
